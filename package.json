    "prettier": "^2.1.2",
    "react-typed": "^1.2.0"
  },
  "dependencies": {
    "react-blink-text": "^1.0.5",
    "react-bootstrap": "^2.7.0",
    "react-reveal": "^1.2.2"
  },
  "engines": {
    "node": "16.18.0"
  },
